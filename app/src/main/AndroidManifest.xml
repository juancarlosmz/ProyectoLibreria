<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.proyectolibreria">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Devolucion"
            android:label="@string/title_activity_devolucion"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".Prestamo"
            android:label="@string/title_activity_prestamo"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Reserva"
            android:label="@string/title_activity_reserva"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Intercambio"
            android:label="@string/title_activity_intercambio"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Donacion"
            android:label="@string/title_activity_donacion"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>